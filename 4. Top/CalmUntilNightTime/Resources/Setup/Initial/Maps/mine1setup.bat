SET /a %MAP%EXIT%COUNT%NEWXCOORD = 5
SET /a %MAP%EXIT%COUNT%NEWYCOORD = 7

SET /a %MAP%EXITNUM = %COUNT%
GOTO :EOF

:BUILDINGS
SET /a %MAP%BUILDINGTOT = 0
GOTO :EOF

