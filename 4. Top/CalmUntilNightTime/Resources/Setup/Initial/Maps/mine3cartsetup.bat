SET /a %MAP%NEWMAPXCOORD1 = 12
SET /a %MAP%NEWMAPYCOORD1 = 5
GOTO :EOF

:UNDERCART
SET /a %MAP%MINECARTXCOORD = 4
SET /a %MAP%MINECARTYCOORD = 7
SET %MAP%UNDERCART=TRACKLEFTRIGHT
GOTO :EOF