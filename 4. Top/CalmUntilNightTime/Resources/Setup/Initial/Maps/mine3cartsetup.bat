SET MAP=MINE3
CALL :POSITIONS
CALL :NEWMAP
CALL :UNDERCART
GOTO :EOF

:POSITIONS
SET /a COUNT = 0
:POSITION1
SET /a COUNT = %COUNT% + 1
SET /a %MAP%CARTPOSITION%COUNT%YCOORD = 5
SET /a %MAP%CARTPOSITION%COUNT%XCOORD = 1
SET /a %MAP%CARTPOSITION%COUNT%CHEST = 0
:POSITION2
SET /a COUNT = %COUNT% + 1
SET /a %MAP%CARTPOSITION%COUNT%YCOORD = 5
SET /a %MAP%CARTPOSITION%COUNT%XCOORD = 2
SET /a %MAP%CARTPOSITION%COUNT%CHEST = 0
:POSITION3
SET /a COUNT = %COUNT% + 1
SET /a %MAP%CARTPOSITION%COUNT%YCOORD = 6
SET /a %MAP%CARTPOSITION%COUNT%XCOORD = 2
SET /a %MAP%CARTPOSITION%COUNT%CHEST = 0
:POSITION4
SET /a COUNT = %COUNT% + 1
SET /a %MAP%CARTPOSITION%COUNT%YCOORD = 7
SET /a %MAP%CARTPOSITION%COUNT%XCOORD = 2
SET /a %MAP%CARTPOSITION%COUNT%CHEST = 0
:POSITION5
SET /a COUNT = %COUNT% + 1
SET /a %MAP%CARTPOSITION%COUNT%YCOORD = 7
SET /a %MAP%CARTPOSITION%COUNT%XCOORD = 3
SET /a %MAP%CARTPOSITION%COUNT%CHEST = 0
:POSITION6
