@ECHO OFF
CALL "%MUSICPATH%\stopmusic.bat"
SET GIRLNAME=Sasha
SET GIRLNAMECAP=SASHA
SET /a GIRLNAMELENGTH = 5
SET GIRLHOUSEVAR=К   %GIRLNAME%'s   К
SET GIRLHOUSEVAR1=!GIRLHOUSEVAR:~0,5!
SET GIRLHOUSEVAR2=!GIRLHOUSEVAR:~5,5!
SET GIRLHOUSEVAR3=!GIRLHOUSEVAR:~10!
SET FRIENDNAME=Ron
SET FRIENDNAMECAP=RON
SET /a FRIENDNAMELENGTH = 3
SET FRIENDHOUSEVAR=К    %FRIENDNAME%'s    К
SET FRIENDHOUSEVAR1=!FRIENDHOUSEVAR:~0,5!
SET FRIENDHOUSEVAR2=!FRIENDHOUSEVAR:~5,5!
SET FRIENDHOUSEVAR3=!FRIENDHOUSEVAR:~10!
SET /a SCENETOT = 24
SET /a SKIP = 0
SET /a COUNT = 0
:LOOPSTART
SET /a COUNT = %COUNT% + 1
CLS
ECHO.
CALL :SCENE%COUNT%IMAGE
CALL :SCENE%COUNT%TEXT
CALL :CONTROLS
IF %CUTSCENEPROGRESSION% EQU 1 (
	CHOICE /C:012 /N /T:%WAITTIME% /D:1
) ELSE (
	CHOICE /C:012 /N /T:9999 /D:1
)
IF %ERRORLEVEL% EQU 1 (
	SET /a SKIP = 1
) ELSE IF %ERRORLEVEL% EQU 3 (
	IF %COUNT% GTR 1 (
		SET /a COUNT = %COUNT% - 2
	) ELSE (
		SET /a COUNT = %COUNT% - 1
	)
)
IF %SKIP% EQU 1 (
	CALL :CONFIRM
)
IF %SKIP% EQU 1 (
	GOTO :END
)
IF %COUNT% LSS %SCENETOT% (
	GOTO :LOOPSTART
)
:END
ECHO.
CALL "%INITIALRESOURCEPATH%\story.bat"
CALL "%NEWRESOURCEPATH%\initialstory.bat"
SET /a HOMETOWNQUESTPHASE = 0
GOTO :EOF

:CONTROLS
ECHO.
ECHO. 1 - Fast forward
ECHO. 2 - Rewind
ECHO. 0 - Pause, with the option to skip
ECHO.
GOTO :EOF

:CONFIRM
CLS
ECHO.
ECHO Paused. Do you want to skip this cutscene?
ECHO If you haven't already assigned names to the characters then they'll be set to Ron and Sasha.
ECHO.
ECHO. 1 - Yes
ECHO. 0 - No, continue
ECHO.
SET /P SKIP=
IF "%SKIP%" EQU "1" (
	GOTO :EOF
) ELSE IF "%SKIP%" EQU "0" (
	SET /a COUNT = %COUNT% - 1
	GOTO :EOF
) ELSE (
	GOTO :CONFIRM
)
GOTO :EOF


